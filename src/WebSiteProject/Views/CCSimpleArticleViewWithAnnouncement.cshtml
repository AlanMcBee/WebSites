@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "_CodeCharmMasterLayoutTemplate.cshtml";
}
<article>
    <header>
        <h1>@Umbraco.Field("pageName")</h1>
    </header>
    @Umbraco.Field("articleText")
</article>
@{
    var startDate = Model.Content.GetPropertyValue<DateTime>("announcement1Start");
    var endDate = Model.Content.GetPropertyValue<DateTime>("announcement1Last");
}
@if (startDate <= DateTime.Now.Date && endDate >= DateTime.Now.Date)
{
    <article>
        @Umbraco.Field("announcement1")
    </article>
}