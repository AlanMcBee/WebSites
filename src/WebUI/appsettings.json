{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AzureKeyVaultEndpoint": "https://kv-tsw-prd-scu.vault.azure.net",
  "ConnectionStrings": {
    "UmbracoDbDSN": ""
  },
  "KeyVaultMappings": {
    "ConnectionStrings__UmbracoDbDSN": "kvsscs-tsw-prd-scu",
    "Umbraco__Storage__AzureBlob__Media__ConnectionString": "kvssvs-stc-tsw-prd-scu"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "c2a611f6-d82f-487f-a265-7c24cf1f6710",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Hosting": {
        "LocalTempStorageLocation": "EnvironmentTemp",
        "Debug": false
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "@Microsoft.KeyVault(SecretUri=https://kv-tsw-prd-scu.vault.azure.net/secrets/kvssvs-stc-tsw-prd-scu)",
          "ContainerName": "tsw"
        }
      }
    }
  }
}
