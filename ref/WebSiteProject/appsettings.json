{
  "$schema": "appsettings-schema.json",
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "fc96c53a-cd10-4d71-a169-6a13ba1d9ba5",
        "SanitizeTinyMce": true,
        "TimeOut": "24:00:00"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "@Microsoft.KeyVault(SecretUri=https://kv-tsw-prd-scu.vault.azure.net/secrets/kvssvs-stc-tsw-prd-scu)",
          "ContainerName": "tsw"
        }
      }
    }
  },
  "AzureKeyVaultEndpoint": "https://kv-tsw-prd-scu.vault.azure.net/",
  "ConnectionStrings": {
    "UmbracoDbDSN": ""
  },
  "KeyVaultMappings": {
    "ConnectionStrings__UmbracoDbDSN": "kvsscs-tsw-prd-scu",
    "Umbraco__Storage__AzureBlob__Media__ConnectionString": "kvssvs-stc-tsw-prd-scu"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  }
}
